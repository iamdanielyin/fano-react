  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome",
      "webRoot": "${workspaceRoot}",
      "url": "http://localhost:8000",
      "sourceMaps": true,
      "smartStep": true,
      "showAsyncStacks": true,
      "skipFiles": [
          "node_modules/**"
      ],
      "sourceMapPathOverrides": {
          "webpack:///*": "${webRoot}/*"
      }
    }
  ]
}